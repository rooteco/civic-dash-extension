{
  "name": "CivicDash",
  "description": "Extension that redirects new tabs to the CivicDash website",
  "version": "1.0.1",
  "action": {},
  "manifest_version": 3,
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png",
    "16": "icons/icon16.png"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "chrome:/"
  ],
  "chrome_url_overrides": {
    "newtab": "redirect.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
